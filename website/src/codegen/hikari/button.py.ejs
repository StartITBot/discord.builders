<%
const buttonStyle = {
    1: "hikari.components.ButtonStyle.PRIMARY",
    2: "hikari.components.ButtonStyle.SECONDARY",
    3: "hikari.components.ButtonStyle.SUCCESS",
    4: "hikari.components.ButtonStyle.DANGER"
}

const className = data.comp.style === 5 ? 'hikari.impl.LinkButtonBuilder' : 'hikari.impl.InteractiveButtonBuilder'

-%>
<%-className%>(
<% if (data.comp.style === 5) { -%>
    url=<%=data.comp.url%>,
<% } else { -%>
    style=<%-buttonStyle[data.comp.style]%>,
<% } -%>
<% if (data.comp.label) { -%>
    label=<%=data.comp.label%>,
<% } -%>
<% if (data.comp.emoji) { -%>
    emoji=<%- include('/hikari/emoji.py.ejs', {emoji: data.comp.emoji}) -%>,
<% } -%>
<% if (data.comp.disabled) { -%>
    disabled=True,
<% } -%>
<% if (data.comp.custom_id) { -%>
    custom_id=<%=data.comp.custom_id%>,
<% } -%>
)<% _%>
