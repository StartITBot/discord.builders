<%
const builderConstructor = {
    3: 'ComponentType.SelectMenu',
    5: 'ComponentType.UserSeelct',
    6: 'ComponentType.RoleSelect',
    7: 'ComponentType.MentionableSelect',
    8: 'ComponentType.ChannelSelect',
};
-%><% _%>
new SelectMenuBuilder()
    .WithType(<%- builderConstructor[data.comp.type] %>)
    .WithCustomId(<%= data.comp.custom_id %>)<% _%>
<%_ %><% if (data.comp.disabled) { %>
    .WithDisabled(<%- data.comp.disabled ? 'true' : 'false' %>)<% _%>
<% } %><% _%>
<%_ %><% if (data.comp.placeholder) { %>
    .WithPlaceholder(<%= data.comp.placeholder %>)<% _%>
<% } %><% _%>
<%_ %><% if (data.comp.min_values && data.comp.min_values !== 1) { %>
    .WithMinValues(<%= data.comp.min_values %>)<% _%>
<% } %><% _%>
<%_ %><% if (data.comp.max_values && data.comp.max_values !== 1) { %>
    .WithMaxValues(<%= data.comp.max_values %>)<% _%>
<% } %><% _%>
<%_ %><% if (data.comp.options) { %>
    .WithOptions([
<%_ %><% data.comp.options.forEach(function(comp, i){ -%>
<%- indent(include('/discord-net/selectmenu_option.cs.ejs', {comp: comp}), 8); %><% if (i !== data.comp.options.length - 1) { %>,<% } %>
<% }); -%>
    ])<% _%>
<% } %><% _%>
